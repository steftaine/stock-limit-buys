{
    "rewrites": [
        {
            "source": "/api/proxy",
            "destination": "/api/proxy.py"
        }
    ],
    "headers": [
        {
            "source": "/(.*)\\.js",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-cache, no-store, must-revalidate"
                }
            ]
        }
    ]
}